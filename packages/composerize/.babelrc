{
  "presets": [
    ["flow"],
    ["env", {
      "targets": {
        "node": "current",
        "browsers": ["last 5 versions", "safari >= 7"]
      },
      "modules": false
    }]
  ],
  "plugins": [
    "external-helpers"
  ],
  "env": {
    "test": {
      "presets": [
        ["env", {
          "modules": "commonjs"
        }]
      ]
    }
  }
}
